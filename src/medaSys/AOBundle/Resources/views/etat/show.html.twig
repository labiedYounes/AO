{% extends '::base.html.twig' %}

{% block body -%}
    <h1>etat</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Ref</th>
                <td>{{ entity.ref }}</td>
            </tr>
            <tr>
                <th>Valuesarray</th>
                <td>{{ entity.valuesArray }}</td>
            </tr>
            <tr>
                <th>Ordernum</th>
                <td>{{ entity.orderNum }}</td>
            </tr>
            <tr>
                <th>Displayedstring</th>
                <td>{{ entity.displayedString }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('etat') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('etat_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
