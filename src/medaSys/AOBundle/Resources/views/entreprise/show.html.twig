{% extends '::base.html.twig' %}

{% block body -%}
    <h1>entreprise</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nomcontact</th>
                <td>{{ entity.nomContact }}</td>
            </tr>
            <tr>
                <th>Nomentreprise</th>
                <td>{{ entity.nomEntreprise }}</td>
            </tr>
            <tr>
                <th>Secteur</th>
                <td>{{ entity.secteur }}</td>
            </tr>
            <tr>
                <th>Forme</th>
                <td>{{ entity.forme }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ entity.adresse }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ entity.telephone }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ entity.fax }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ entity.mail }}</td>
            </tr>
            <tr>
                <th>Site</th>
                <td>{{ entity.site }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('entreprise') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('entreprise_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
