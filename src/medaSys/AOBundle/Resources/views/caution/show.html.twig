{% extends '::base.html.twig' %}

{% block body -%}
    <h1>caution</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Datedemande</th>
                <td>{{ entity.dateDemande|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Dateretour</th>
                <td>{{ entity.dateRetour|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('caution') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('caution_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
